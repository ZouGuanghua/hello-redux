{
    "name": "app",
    "version": "0.0.1",
    "description": " ",
    "main": "server.js",
    "devDependencies": {
        "ava": "^0.15.2",
        "babel-cli": "^6.9.0",
        "babel-core": "^6.7.7",
        "babel-loader": "^6.2.4",
        "babel-polyfill": "^6.9.0",
        "babel-preset-es2015": "^6.6.0",
        "babel-preset-react": "^6.5.0",
        "babel-preset-stage-1": "^6.5.0",
        "cross-env": "^1.0.8",
        "css-loader": "^0.19.0",
        "less": "^2.6.1",
        "less-loader": "^2.2.3",
        "lodash": "^4.11.2",
        "react": "^15.1.0",
        "react-addons-test-utils": "^15.0.1",
        "react-codemirror": ">=0.1.2",
        "react-dom": ">=15.0.1",
        "react-hot-loader": "^1.2.8",
        "react-jade": "^2.5.0",
        "react-jade-loader": "^0.1.0",
        "react-redux": "^4.4.5",
        "react-router": "^2.4.0",
        "react-router-redux": "^4.0.4",
        "redux": "^3.5.2",
        "redux-localstorage": "^0.4.0",
        "redux-logger": "^2.6.1",
        "redux-thunk": "^2.0.1",
        "style-loader": "^0.12.4",
        "tcomb": "^3.0.0",
        "tcomb-react": "^0.9.0",
        "webpack": "^1.13.1",
        "webpack-dev-server": "^1.13.0",
        "wolfy87-eventemitter": "^4.3.0"
    },
    "scripts": {
        "start": "node server.js",
        "release": "NODE_ENV=production webpack",
        "build": "NODE_ENV=development webpack",
        "clear": "",
        "build-common": "cd common && NODE_ENV=development webpack --config ./webpack.config.js",
        "release-common": "cd common && NODE_ENV=production webpack --config ./webpack.config.js",
        "test": "ava",
        "testwatch": "ava --watch"
    },
    "babel": {
        "plugins": [
            "transform-async-to-generator"
        ],
        "presets": [
            "es2015",
            "stage-1",
            "react"
        ],
        "env": {
            "development": {
                "sourceMaps": "inline"
            }
        }
    },
    "ava": {
        "babel": "inherit",
        "source": [
            "**/*.{js,jsx,jade}",
            "!build/**/*"
        ],
        "require": [
            "babel-polyfill",
            "babel-register",
            "./tools/ignore_style"
        ]
    },
    "license": "MIT",
    "dependencies": {}
}